#include <stdio.h>
#include <string.h>

int main() {
    FILE *fp = fopen("demo.txt", "r");
    char line[256];
    char keyword[] = "UNIX";

    while (fgets(line, sizeof(line), fp)) {
        if (strstr(line, keyword)) {
            printf("%s", line);
        }
    }
    fclose(fp);

    return 0;
}
